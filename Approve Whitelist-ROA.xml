    $XmlWriter.WriteElementString('process', '*')
    $XmlWriter.WriteElementString('operation', 'rx')
    $XmlWriter.WriteElementString('learningmode', 'no')
    $xmlWriter.WriteStartElement('accesscontrol')
    $XmlWriter.WriteAttributeString('access_mode', "or")
    $XmlWriter.WriteAttributeString('zone_mode', "or")
    $xmlWriter.WriteStartElement('access')
    $XmlWriter.WriteElementString('type', 'global')
    $xmlWriter.WriteEndElement()
    $xmlWriter.WriteEndElement()
    $xmlWriter.WriteStartElement('hashes')
    $XmlWriter.WriteAttributeString('allowany', "no")
    $xmlWriter.WriteStartElement('hash')    
    $XmlWriter.WriteAttributeString('type', "SHA-256")
    $XmlWriter.WriteElementString('value', '$[hash]')
    $XmlWriter.WriteElementString('mod', $Now)
    $xmlWriter.WriteEndElement()
    $xmlWriter.WriteEndElement()
    $xmlWriter.WriteElementString('guid',$guid)
    $xmlWriter.WriteElementString('enabled','yes')
    $xmlWriter.WriteEndElement()
$xmlWriter.WriteEndDocument()
$xmlWriter.Flush()
$xmlWriter.Close()</source>
